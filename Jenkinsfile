        echo 'Checkout completed'
      }
    }

    stage('Build') {
      steps {
        echo 'Build completed'
      }
    }

    stage('Test') {
      steps {
        echo 'Test Completed'
      }
    }

    stage('Deploy') {
      steps {
        echo 'Deploy Completed'
      }
    }

  }
}